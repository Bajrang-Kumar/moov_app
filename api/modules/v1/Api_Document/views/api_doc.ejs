	<link rel="shortcut icon" href="<%= GLOBALS.BASE_URL_WITHOUT_PORT + GLOBALS.LOGO%>">
	<title>
		<%= GLOBALS.APP_NAME %> API LIST
	</title>
	<style type="text/css">
		.medium-text {
			font-size: 18px;
			color: white;
		}

		.js div#preloader {
			position: fixed;
			left: 0;
			top: 0;
			z-index: 999;
			width: 100%;
			height: 100%;
			overflow: visible;
			background: #fff url('<%= GLOBALS.BASE_URL_WITHOUT_API %>ripple.gif') no-repeat center center;
		}
	</style>
</head>

