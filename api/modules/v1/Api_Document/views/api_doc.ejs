							<tr>
								<td><b>Server URL </b></td>
								<td><img src='<%= GLOBALS.BASE_URL_WITHOUT_API+ GLOBALS.ARROW_IMAGE %>'
										style="width: 40px; border-radius: 5px;">&nbsp;&nbsp;
									<b>
										<%= GLOBALS.PORT_BASE_URL %>api/v1/
									</b>
								</td>
							</tr>
							<tr>
								<td><b>Header Parameters </b></td>
								<td><img src='<%= GLOBALS.BASE_URL_WITHOUT_API+ GLOBALS.ARROW_IMAGE %>'
										style="width: 40px; border-radius: 5px;">&nbsp;&nbsp;
									<b>
										API-KEY, TOKEN
									</b>
								</td>
							</tr>
							<tr>
								<td><b>API-KEY </b></td>
								<td><img src='<%= GLOBALS.BASE_URL_WITHOUT_API+ GLOBALS.ARROW_IMAGE %>'
										style="width: 40px; border-radius: 5px;">&nbsp;&nbsp;
									<b>
										<%= GLOBALS.API_KEY %> (vyYSDnpAqG5YESnqyj+K/w==)
									</b>
								</td>
							</tr>
							<tr>
								<td><b>KEY </b></td>
								<td><img src='<%= GLOBALS.BASE_URL_WITHOUT_API+ GLOBALS.ARROW_IMAGE %>'
										style="width: 40px; border-radius: 5px;">&nbsp;&nbsp;
									<b>
										<%= GLOBALS.KEY %>
									</b>
								</td>
							</tr>
							<tr>
								<td><b>IV </b></td>
								<td><img src='<%= GLOBALS.BASE_URL_WITHOUT_API+ GLOBALS.ARROW_IMAGE %>'
