					<br> -->

					<div
						style="position: relative; display: inline-block; width: 10rem; height: 10rem; border-radius: 50%; padding: 3px; background: linear-gradient(to right, red, darkblue);">
						<img src="<%= GLOBALS.BASE_URL_WITHOUT_PORT + GLOBALS.LOGO %>"
							style=" width: 100%; height: 100%; border-radius: 50%; display: block; background: white; padding: 3px; box-sizing: border-box; ">
						<strong style="font-size:24px;">
							<%= GLOBALS.APP_NAME %> API LIST
						</strong><br><br>
					</div>


					<v-alert :value="true" outline color="white" style="padding:5px;">
						<v-chip color="orange" text-color="">
							<v-avatar>
								<v-icon>bookmark</v-icon>
							</v-avatar>
							<b>TOTAL {{messages.length}} APIs</b>
						</v-chip>
					</v-alert>

					<v-alert :value="true" outline color="white" style="padding:5px;">
						<v-chip color="light-blue" text-color="">
							<v-avatar>
								<v-icon>label_important</v-icon>
							</v-avatar>
							<b>If Device Token not get, pass 0 only</b>
						</v-chip>
						<v-chip color="light-blue" text-color="">
							<v-avatar>
								<v-icon>label_important</v-icon>
							</v-avatar>
							<b>Upload images on S3 bucket</b>
						</v-chip>
					</v-alert>
				</v-flex>
				<v-flex style="margin:0px auto; padding:0px;" center right sm2 xs2 md2 lg2 xl2>

					<div id="main1" style="margin:0px auto; position:sticky;">
